<Window x:Class="Projekt_koncowy.info"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt_koncowy"
        mc:Ignorable="d"
        Background="Black"
        MinHeight="450"
        MinWidth="800"
        Title="info" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="39*">

            </RowDefinition>
            <RowDefinition Height="178*">

            </RowDefinition>
        </Grid.RowDefinitions>
        <TextBox x:Name="Head" FontFamily="Elephant" HorizontalAlignment="Stretch" TextWrapping="Wrap" Text="Lista wszystkich pozycji na gieldzie kryptowalut" VerticalAlignment="Stretch" Margin="10,10,10,5" Background="#FFF34E4E" FontSize="24" TextAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
        <TextBox x:Name="Crypto_Text" HorizontalAlignment="Stretch" Grid.Row="1" TextWrapping="Wrap" VerticalAlignment="Stretch" Margin="10,5,10,10" FontFamily="Dubai Medium" FontSize="20" Background="#FF5AC3F7"/>
       
    </Grid>

</Window>
